<?xml version="1.0" encoding="UTF-8"?>

<namespace xmlns="http://www.mulesoft.org/schema/mule/tooling.attributes" xmlns:mule="http://www.mulesoft.org/schema/mule/core" url="http://www.mulesoft.org/schema/mule/keycloak" prefix="keycloak">
  <global-cloud-connector versions="[3.5.0,8.0.0]" caption="Keycloak" description="" icon="" localId="parentConfig" image="" abstract="true" extends="http://www.mulesoft.org/schema/mule/keycloak/abstractKeycloakConnectorGeneric"></global-cloud-connector>
  <global-cloud-connector versions="[3.5.0,8.0.0]" caption="Keycloak: Configuration" description="Global Configuration configuration information." icon="icons/small/keycloak-connector-24x16.png" localId="config" image="icons/large/keycloak-connector-48x32.png" abstract="false" extends="http://www.mulesoft.org/schema/mule/keycloak/parentConfig" connectivityTesting="off" metaData="static" metaDataKeyDisplay="DISPLAY_NAME" supportsUserDefinedMetaData="false">
    <attribute-category caption="General" description="Keycloak configuration properties.">
      <group id="keycloakGenericProperties" caption="Basic Settings">
        <name name="name" caption="Name" description="Give a name to this configuration so it can be later referenced by config-ref." required="true"></name>
      </group>
      <group id="general" caption="General">
        <string defaultValue="http://localhost" name="keycloakUrl" caption="Keycloak Url" description="Keycloak Server URL." supportsExpressions="false" required="false" javaType="java.lang.String"></string>
        <integer defaultValue="8080" min="0" step="1" name="keycloakPort" caption="Keycloak Port" description="Keycloak Server Port." supportsExpressions="false" required="false" javaType="int"></integer>
        <string name="adminPassword" caption="Admin Password" description="Keycloak administration User password." supportsExpressions="false" required="true" javaType="java.lang.String"></string>
        <string name="adminUser" caption="Admin User" description="Keycloak administration User." supportsExpressions="false" required="true" javaType="java.lang.String"></string>
        <string name="realm" caption="Realm" description="Keycloak Realm to administrate." supportsExpressions="false" required="true" javaType="java.lang.String"></string>
      </group>
    </attribute-category>
  </global-cloud-connector>
  <cloud-connector caption="Abstractkeycloakconnectorgeneric" description="Interact with Keycloak." icon="icons/small/keycloak-connector-24x16.png" localId="abstractKeycloakConnectorGeneric" image="icons/large/keycloak-connector-48x32.png" abstract="true">
    <attribute-category caption="General" description="General.">
      <group id="abstractKeycloakConnectorGeneric" caption="Basic Settings">
        <globalRef requiredType="http://www.mulesoft.org/schema/mule/keycloak/config" name="config-ref" caption="Connector Configuration" description="Specify which configuration to use for this invocation." required="true"></globalRef>
      </group>
    </attribute-category>
  </cloud-connector>
  <cloud-connector versions="[3.5.0,8.0.0]" aliasId="org.mule.tooling.ui.modules.core.pattern.keycloak" caption="Keycloak" description="" icon="icons/small/keycloak-connector-24x16.png" localId="keycloak-connector" image="icons/large/keycloak-connector-48x32.png" extends="http://www.mulesoft.org/schema/mule/keycloak/abstractKeycloakConnectorGeneric">
    <attribute-category caption="General" description="General.">
      <group id="keycloakConnectorGeneric" caption="Basic Settings">
        <modeSwitch alwaysCombo="true" name="Keycloak operations to execute" caption="Operation" description="Operation.">
          <no-operation abstractElement="http://www.mulesoft.org/schema/mule/keycloak/abstractKeycloakConnectorGeneric" versions="[3.5.0,8.0.0]" connector-name="processor"></no-operation>
          <mode modeId="http://www.mulesoft.org/schema/mule/keycloak/get-user-by-id" modeLabel="Get user by id"></mode>
        </modeSwitch>
      </group>
    </attribute-category>
  </cloud-connector>
  <cloud-connector returnType="void" caption="Get User By Id" icon="icons/small/keycloak-connector-24x16.png" localId="get-user-by-id" image="icons/large/keycloak-connector-48x32.png" abstract="true" extends="http://www.mulesoft.org/schema/mule/keycloak/abstractKeycloakConnectorGeneric" metaData="static">
    <attribute-category caption="General" description="General.">
      <group id="general" caption="General">
        <string name="id" caption="Id" description="The id of the user which should be returned." required="true" javaType="java.lang.String"></string>
      </group>
    </attribute-category>
  </cloud-connector>
</namespace>